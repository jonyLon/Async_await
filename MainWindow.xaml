<Window x:Class="Async_await.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Async_await"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button x:Name="Generate" Click="Generate_Click">Generate</Button>
            <TextBox Width="100" x:Name ="number"></TextBox>
        </ToolBar>
        <ProgressBar DockPanel.Dock="Bottom" Height="20" IsIndeterminate="True"></ProgressBar>
        <ListBox x:Name="list">
            
        </ListBox>
    </DockPanel>
</Window>
